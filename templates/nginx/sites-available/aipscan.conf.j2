#Ansible managed
server {

   listen {{ aipscan_listen_port }}; 
   location / {
       proxy_set_header Host $http_host;
       proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
       proxy_redirect off;
       proxy_buffering off;
       proxy_read_timeout 172800s;
       proxy_pass http://localhost:4573;
       
   }
 

}
